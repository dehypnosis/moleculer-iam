{
  "name": "moleculer-iam",
  "version": "0.2.7",
  "description": "Centralized IAM module for moleculer.\nIncluding a certified OIDC provider and an Identity provider for user profile, credentials, and custom claims management.\nCustom claims could be defined/updated by declarative schema which contains claims validation and migration strategy.",
  "author": "QMIT Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/qmit-pro/moleculer-iam.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "nodemon",
    "build": "rm -rf dist && tsc",
    "lint": "tslint -p tsconfig.json",
    "deps": "yarn-check -u",
    "coverall": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test": "yarn test-with-examples --testPathIgnorePatterns=src/examples",
    "test-with-examples": "node --trace-warnings node_modules/.bin/jest --coverage --detectOpenHandles --forceExit"
  },
  "bin": {
    "generate-jwks": "./bin/generate-jwks.js"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/**/*.spec.ts"
    ],
    "exec": "ts-node src/examples/index.ts"
  },
  "keywords": [
    "QMIT",
    "moleculer"
  ],
  "dependencies": {
    "accept-language-parser": "^1.5.0",
    "awesome-phonenumber": "^2.23.0",
    "bcrypt": "^3.0.7",
    "change-case": "^4.1.1",
    "dataloader": "^2.0.0",
    "fastest-validator": "^1.0.0-beta4",
    "i18next": "^19.3.2",
    "jose": "latest",
    "kleur": "^3.0.3",
    "koa-bodyparser": "^4.2.1",
    "koa-compose": "^4.1.0",
    "koa-helmet": "^5.2.0",
    "koa-json": "^2.0.2",
    "koa-locale": "^1.3.0",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^4.0.0",
    "koa-no-trailing-slash": "^2.1.0",
    "koa-passport": "^4.1.3",
    "koa-router": "8.0.7",
    "koa-static-cache": "^5.1.2",
    "koajs-nocache": "^2.0.3",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "moleculer": "^0.14.1",
    "moleculer-iam-app-renderer": "*",
    "moment": "^2.24.0",
    "mysql2": "^2.0.1",
    "object-hash": "^2.0.1",
    "oidc-provider": "^6.18.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-kakao": "^1.0.0",
    "passport-koa": "^1.0.1",
    "request-promise-native": "^1.0.8",
    "sequelize": "^5.21.2",
    "terser": "^4.4.3",
    "umzug": "^2.2.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/accept-language-parser": "^1.5.1",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.24",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-json": "^2.0.18",
    "@types/koa-morgan": "^1.0.4",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-passport": "^4.0.2",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/koa-static-cache": "^5.1.0",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "13.7.1",
    "@types/object-hash": "^1.3.0",
    "@types/passport": "^1.0.2",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/passport-kakao": "^0.2.0",
    "@types/request-promise-native": "^1.0.17",
    "@types/terser": "^3.12.0",
    "@types/umzug": "^2.2.2",
    "@types/uuid": "^3.4.6",
    "benchmarkify": "^2.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.0.6",
    "jest": "^25.1.0",
    "jest-cli": "25.1.0",
    "moleculer-qmit": "^1.0.12",
    "moleculer-repl": "^0.6.2",
    "nodemon": "2.0.2",
    "sequelize-cli": "^5.5.1",
    "ts-jest": "25.2.0",
    "ts-node": "^8.5.4",
    "tslint": "6.0.0",
    "typescript": "^3.7.3",
    "yarn-check": "^0.0.3"
  },
  "engines": {
    "node": ">= 10.x.x"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "transform": {
      "^.+\\.(ts)$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/src/**/*.spec.(ts|js)"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    }
  }
}
